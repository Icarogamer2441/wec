module funcs_vars_etc;

fn fibonacci(n: i32) -> i32 {
    if n <= 0 {
        return 0;
    } else if n == 1 {
        return 1;
    } else {
        return fibonacci(n - 1) + fibonacci(n - 2);
    }
}

fn main() -> i32 {
    let x: i32 = 10;
    let y: i32 = 20;
    wec.print("x: ", x);
    wec.print("y: ", y);
    wec.print("fibonacci(10): ", fibonacci(10));
    return 0;
}